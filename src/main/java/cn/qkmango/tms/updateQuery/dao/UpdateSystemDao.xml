<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.qkmango.tms.updateQuery.dao.UpdateSystemDao">


    <update id="updateSystemKeyValue">
        update t_system
        set `key`=#{newKey},
            value=#{newValue}
        where `key` = #{key}
    </update>

</mapper>
